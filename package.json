{
  "name": "melon-split-pane",
  "version": "1.1.0",
  "description": "Split Pane Component for React",
  "main": "lib/SplitPane.js",
  "browser": "lib/SplitPane.js",
  "module": "src/SplitPane.js",
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --config tools/webpack.dev.js",
    "build": "rimraf lib && NODE_ENV=production gulp build",
    "lint": "fecs src",
    "test": "NODE_ENV=test karma start ./test/karma.local.conf.js",
    "test-ci": "NODE_ENV=test karma start ./test/karma.travis.conf.js",
    "coveralls": "cat test/coverage/lcov/lcov.info | ./node_modules/.bin/coveralls",
    "docs": "esdoc -c esdoc.json"
  },
  "keywords": [
    "melon",
    "react",
    "split",
    "splitpane"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-melon/melon-split-pane.git"
  },
  "authors": [
    "ludafa <ludafa@outlook.com>"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "melon": ">=0.6.0-alpha.3",
    "melon-core": ">=0.5.2"
  },
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "coveralls": "^2.11.12",
    "css-loader": "^0.24.0",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "fecs": "^0.8.6",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^1.6.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",
    "melon": ">=0.6.0-alpha.3",
    "melon-core": ">=0.5.2",
    "nib": "^1.1.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "watchify": "^3.7.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  }
